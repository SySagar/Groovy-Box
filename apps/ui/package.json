{
  "name": "@groovy-box/ui",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.mjs",
  "typings": "dist/lib/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./dist/*": "./dist/*"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "release": "changeset publish",
    "pub:beta": "npm run build && npm publish --no-git-checks --access public --tag beta",
    "test": "jest",
    "test-cov": "jest --coverage",
    "format": "prettier --write lib/**/* stories/**/* && git add -A .",
    "lint": "eslint ./lib --ext .ts,.tsx",
    "lint-fix": "eslint --fix .",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "SySagar",
  "size-limit": [
    {
      "path": "dist/groovy-box.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/groovy-box.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@changesets/cli": "^2.27.1",
    "@radix-ui/react-label": "^2.0.2",
    "@size-limit/preset-small-lib": "^10.0.2",
    "@storybook/addon-controls": "^7.6.20",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.5.3",
    "@storybook/builder-vite": "^8.2.1",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^8.2.1",
    "@storybook/react-webpack5": "7.5.3",
    "@storybook/testing-react": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "clsx": "^2.1.0",
    "css-loader": "^6.10.0",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.4",
    "radix-ui": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^10.0.2",
    "storybook": "^8.2.1",
    "storybook-css-modules": "^1.0.8",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "lucide-react": "^0.377.0",
    "tailwind-merge": "^2.3.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0"
  }
}
